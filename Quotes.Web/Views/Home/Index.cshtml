@model IEnumerable<Quotes.Entities.Mood>

@{
    ViewBag.Title = "All moods";
}

<h2>@ViewBag.Title</h2>


<table>
    <tr>
        <th>
            Mood 
        </th>
        <th></th>
        <th></th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.moodDesc)
        </td>
        <td>
            |
            @*@Html.ActionLink("Details", "Details", new { id=item.moodID },null) |*@
            @Html.ActionLink("View Quotes ", "Mood", new { id = item.moodID }, null) 
        </td>
    </tr>
}

</table>
